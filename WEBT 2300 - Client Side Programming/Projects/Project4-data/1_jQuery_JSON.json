<html>
<head>
<meta charset="utf-8">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script>
$(document).ready(function(){
    
        $.getJSON("webt-2300.json", function(result){
            $.each(result, function(i, field){
                $("p").append(field.last_name + " " + field.first_name + " " + field.role + "<br />");
            });
        });
    
});
</script>
</head>
<body>


<p></p><!-- This is the div the data will append to-->

</body>
</html>